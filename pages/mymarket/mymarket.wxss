
page {
  background: #1C162E;
  overflow: hidden;
}
.content {
  width: 100%;
  overflow: hidden;
  margin-top:160rpx;
  padding-bottom: 200rpx;
}
.nav{
  width: 100%;
  height: 168rpx;
  background: #201A32;
  position: fixed;
  top: 0;
  z-index: 10;
  color: white;
  overflow: hidden;
}
.place-holder{
  font-size: 28rpx;
  color: #6B657A;
}
.nav .nav-title{
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  margin-top:80rpx ;
}
.nav .nav-title van-image{
  margin-left:30rpx ;
}
.nav .nav-title text{
  margin-left: 230rpx;
}
.search-market,
.list {
  width: 690rpx;
  background:rgba(44,37,62,0.6);
  margin-top: 24rpx;
  margin-left: 30rpx;
  border-radius: 24rpx;
}
.list>text:first-child{
  font-size:34rpx;
  color: #FFFFFF;
}
.search-market {
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position:fixed;
  z-index: 10;
  top: 170rpx;
}

.search-market view {
  margin-right: 15rpx;
  margin-top: 4rpx;
}
input{
  width: 600rpx;
  text-align:left;
  margin-left:30rpx;
  box-sizing:border-box;
  color: #fff;
  font-size: 28rpx;
}
.list>text{
  font-size: 34rpx;
  font-weight: bold;
  margin-left: 30rpx;
  position: absolute;
  margin-top: 20rpx;
}
.footer {
  width: 100%;
  height: 200rpx;
  background:rgba(44,37,62,1);
  box-shadow:0px 4px 20px 0px rgba(0,0,0,0.4);
  border-radius:24px 24px 0px 0px;
  position: fixed;
  bottom: 0;
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: center;
}
